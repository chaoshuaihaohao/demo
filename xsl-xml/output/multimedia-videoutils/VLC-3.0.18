#!/bin/bash

#Start scripts
export LUAC=/usr/bin/luac5.2                          &&
export LUA_LIBS="$(pkg-config --libs lua52)"          &&
export CPPFLAGS="$(pkg-config --cflags lua52)"        &&

BUILDCC=gcc ./configure --prefix=/usr --disable-libva &&

make
make docdir=/usr/share/doc/vlc-3.0.18 install

#End scripts
